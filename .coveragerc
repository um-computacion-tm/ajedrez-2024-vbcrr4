[run]
source =
    game
    test
omit =
    venv/*
    */test*
    */__init__.py
    */__main__.py
[report]
exclude_lines =
    pragma: no cover
    def __repr__
    def setUp
    def test_
    return raw_input
    print
    if __name__ == "__main__":
    if __name__ == '__main__':
    def suite
    __str__
